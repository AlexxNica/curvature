<div id="overviewsDisplayer" class="overviewsHidden" onclick="displayOverviews()">
  <<
</div>
<div id="overviewsContainer" class="overviewsHidden">
</div>

<script>
  var appPage = true

  var overviewsOpen = false;
  function displayOverviews() {
    if(!overviewsOpen){
      document.getElementById("overviewsDisplayer").className = "overviewsVisible";
      document.getElementById("overviewsDisplayer").innerHTML = ">>";
      document.getElementById("overviewsContainer").className = "overviewsVisible";
      overviewsOpen = true;
    }
    else {
      document.getElementById("overviewsDisplayer").className = "overviewsHidden";
      document.getElementById("overviewsDisplayer").innerHTML = "<<";
      document.getElementById("overviewsContainer").className = "overviewsHidden";
      overviewsOpen = false;
    }
  }
</script>

