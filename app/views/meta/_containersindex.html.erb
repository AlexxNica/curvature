<% @containers = ::Container.all %>

<table border="1">
  <tr>
    <th>Name</th>
  </tr>
  <% @containers.each do |container| %>
    <tr>
      <td><%= container.name %></td>
      <td><%= link_to 'Export', export_container_path(container, :format => :json) %></td>
      <td><%= link_to 'Edit', edit_container_path(container), :remote => true %></td>
      <td><%= link_to 'Destroy', container, :method => :delete, :data => { :confirm => 'Are you sure?' } %></td>
    </tr>
  <% end %>

</table>

